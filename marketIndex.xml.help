			<chaosValue>1.00</chaosValue>
		</item>
		<item name="Powerful Chaotic Resonator"><chaosValue>1.00</chaosValue></item>
		<item name="Potent Chaotic Resonator"><chaosValue>1.00</chaosValue></item>
		<item name="Primitive Chaotic Resonator"><chaosValue>1.00</chaosValue></item>
	</class>

